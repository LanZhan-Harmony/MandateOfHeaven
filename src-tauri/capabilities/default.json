{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    {
      "identifier": "http:allow-fetch",
      "allow": [
        {
          "url": "https://prod.beifa.shyingyou.cn/*"
        }
      ]
    },
    {
      "identifier": "http:allow-request",
      "allow": [
        {
          "url": "https://prod.beifa.shyingyou.cn/*"
        }
      ]
    }
  ]
}